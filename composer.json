{
    "name": "shader2k/search-indexer-providers",
    "description": "Providers for shader2k/search-indexer package",
    "type": "library",
    "keywords": [
        "search-indexer",
        "search-indexer provider",
        "eloquent",
        "provider"
    ],
    "authors": [
        {
            "name": "shader2k",
            "email": "shader2k@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
    },
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "shader2k/search-indexer": "^1",
        "illuminate/database": "^5",
        "illuminate/pagination": "^5",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Shader2k\\SearchIndexerProviders\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shader2k\\SearchIndexerProviders\\Tests\\": "tests/",
            "Shader2k\\SearchIndexer\\Tests\\": "vendor/shader2k/search-indexer/tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
